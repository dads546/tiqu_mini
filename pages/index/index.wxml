<view class="container">
  <t-tag class="item" theme="primary"  variant="light" shape="round">dy图集</t-tag>
  <t-input class="item" label="输入地址" placeholder="详情页或分享链接" />
<t-button  class="item" theme="primary" size="small" block>开始</t-button>
</view>
<view class="swiper-container">
  <swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" style="height: 100px;"> 
  <swiper-item wx:for="{{imageUrls}}" wx:key="index">
    <image src="{{item}}" mode="aspectFill"></image>
  </swiper-item>
</swiper>
</view>


