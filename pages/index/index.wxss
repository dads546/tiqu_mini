.main_divider {
  --td-divider-color: rgb(51, 12, 12);
  --td-divider-content-color: gray;
}

.linkBtn {
  width: 100px;
  --td-button-small-font-size: 12px;
}

.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: left;
}
._blank {
  margin-bottom: 50px;
}
.button-group {
  display: flex;
  justify-content: space-between;
}

.round-button {
  display: flex;
  flex-direction: row;
  align-items: left;
  justify-content: center;
  border-radius: 10px;
  padding: 15px;
  margin: 5px;
  width: 180px;
  /*background-color: #f0f0f0;*/
  border: 1px solid #ccc;
}

.button-icon {
  width: 30px;
  height: 30px;
  margin-bottom: 5px;
}

.button_analysis {
  background-color: rgb(209, 178, 178);
}

.btn_text {
  display: block;
  font-size: 10px;
  text-align: left;
  line-height: 1.5em;
  margin-left: 5px;
  color: #666;
}
